{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "strict": true,
    "rootDir": "src",
    "outDir": "dist",
    "allowJs": true,
    "skipLibCheck": true, 
    "resolveJsonModule": true
  },
  "include": [
  "src/**/*.ts"
],
}
